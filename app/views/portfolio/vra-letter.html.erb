<!doctype html>
<html>
<head>
  <title>VRA Letter</title>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
  <meta name="viewport" content="width=device-width, initial-scale=1">
  <link href='http://fonts.googleapis.com/css?family=Nobile|Open+Sans:400,300,600,700' rel='stylesheet' type='text/css'>
  <link rel="stylesheet" type="text/css" href="lib/icomoon/style.css">
  <style type="text/css">
    @font-face {
      font-family: 'icomoon';
      src:url('<%= asset_path 'fonts/vra-icons.eot' %>?vddn2b');
      src:url('<%= asset_path 'fonts/vra-icons.eot' %>?#iefixvddn2b') format('embedded-opentype'),
        url('<%= asset_path 'fonts/vra-icons.woff' %>?vddn2b') format('woff'),
        url('<%= asset_path 'fonts/vra-icons.ttf' %>?vddn2b') format('truetype'),
        url('<%= asset_path 'fonts/vra-icons.svg' %>?vddn2b#icomoon') format('svg');
      font-weight: normal;
      font-style: normal;
    }

    [class^="icon-"], [class*=" icon-"] {
      font-family: 'icomoon';
      speak: none;
      font-style: normal;
      font-weight: normal;
      font-variant: normal;
      text-transform: none;
      line-height: 1;

      /* Better Font Rendering =========== */
      -webkit-font-smoothing: antialiased;
      -moz-osx-font-smoothing: grayscale;
    }

    .icon-facebook:before {
      content: "\ea8c";
    }

    .icon-twitter:before {
      content: "\ea91";
    }

    *,
    *:before,
    *:after {
      box-sizing: border-box;
    }

    html {
      font-family: 'Open Sans', sans-serif;
      font-size: 12pt;
    }

    body {
      background-color: #eee;
      line-height: 1.5;
      margin: 0;
      padding: 0;
    }

    img {
      max-width: 100%;
    }

    p {
      margin: 0;
    }

    /** HELPERS *******************************************************************/

    .container {
      margin: auto;
      max-width: 1280px;
      width: 100%;
    }

    .content {
      background-color: white;
    }

    .screen-reader-text {
      clip: rect(0,0,0,0);
      height: 1px;
      left: 0;
      margin: -1px;
      overflow: hidden;
      position: absolute;
      top: 0;
      width: 1px;
    }

    .float--right {
      float: right;
    }

    .float--right:after {
      content: "";
      clear: right;
    }

    .text--right {
      text-align: right;
    }

    .text--center {
      text-align: center;
    }

    .text--bold {
      font-weight: 700;
    }

    .text--subtle {
      color: #898989;
    }

    .text--large {
      font-size: 1.2em;
      font-weight: 700;
    }

    @media(min-width: 650px) {
      .text--large {
        font-size: 1.38em;
      }
    }

    /** BANNERS *******************************************************************/

    .banner {
      color: white;
      display: block;
    }

    .banner--gradient {
      background-image: radial-gradient(circle at center, #242ca8, #080e6f);
    }

    .banner__heading {
      font-size: 1.2em;
      font-weight: 700;
    }

    .banner__heading--large {
      font-size: 1.5em;
    }

    .banner__text {
    }

    .banner__text p {
      padding: 20px 0;
    }

    .banner__cta {
      color: white;
      display: inline-block;
      font-weight: 700;
      padding-right: 25px;
      position: relative;
      text-transform: uppercase;
    }

    .banner__cta .link {
      color: inherit;
    }

    .banner__cta:before,
    .banner__cta:after {
      content: "";
      background-color: white;
      display: block;
      height: 8px;
      right: 0;
      position: absolute;
      top: 50%;
      width: 6px;
    }

    .banner__cta:before {
      margin-top: -8px;
      transform: skewX(40deg);
    }

    .banner__cta:after {
      margin-top: 0;
      transform: skewX(-40deg);
    }

    @media(min-width: 650px) {
      .banner__heading {
        font-size: 1.38em;
      }

      .banner__heading--large {
        font-size: 2em;
      }

      .banner__text--large {
        font-size: 1.11em;
      }
    }

    /** BUTTONS *******************************************************************/

    .btn {
      display: inline-block;
      line-height: 15px;
      padding: 5px 10px;
      text-decoration: none;
    }

    /** LINKS *********************************************************************/

    .link {
      text-decoration: none;
    }

    /** MENUS *********************************************************************/

    .menu {
      display: block;
      list-style-type: none;
      margin: 0;
      padding: 0;
    }

    .menu__item {
      display: block;
    }

    .menu--horizontal .menu__item {
      display: inline-block;
    }


    /** TOP MENU ******************************************************************/

    .top-menu {
      line-height: 24px;
      padding: 12px 25px 12px 0;
    }

    .top-menu:after {
      content: "";
      clear: both;
      display: table;
    }

    .top-menu .menu__link {
      color: #080e6f;
      text-decoration: none;
    }

    /** HEADER ********************************************************************/

    .header {
      padding: 55px 25px;
      position: relative;
    }

    .logo {
    }

    .main-menu {
      text-align: center;
    }

    .main-menu:after {
      content: "";
      display: table;
      clear: both;
    }

    .main-menu .menu {
      width: 100%;
    }

    .main-menu .menu__item {
      display: block;
      float: left;
      padding: 2px 0;
      width: 50%;
    }

    .main-menu .menu__link,
    .join-btn {
      color: #757575;
      display: block;
      line-height: 20px;
      padding: 4px 0;
      text-decoration: none;
    }

    .main-menu .menu__link.is-selected {
      color: #8dc63f;
      font-weight: 700;
    }

    .join-btn {
      background-color: #a4d662;
      background-image: linear-gradient(to right, #c0e78b, #8dc63f);
      color: white;
    }

    @media(min-width: 450px) {
      .main-menu .menu__item {
        width: 33.3%;
      }
    }

    @media(min-width: 650px) {
      .main-menu {
        text-align: right;
        width: 100%;
      }

      .main-menu .menu__item {
        display: inline-block;
        float: none;
        width: auto;
      }

      .main-menu .menu__link,
      .join-btn {
        margin-left: 10px;
        padding: 10px 0;
      }

      .join-btn {
        padding-left: 25px;
        padding-right: 25px;
      }
    }

    @media(min-width: 1100px) {
      .logo {
        text-align: center;
        width: 45%;
      }

      .main-menu {
        position: absolute;
        right: 25px;
        top: 55px;
        width: auto;
      }
    }

    /** ATTENTION BANNER **********************************************************/

    .attention-banner {
      background-image: url(data:image/jpeg;base64,);
      background-repeat: no-repeat;
      background-size: cover;
      padding: 40px 20px;
    }

    @media(min-width: 1100px) {
      .attention-banner {
        padding: 80px 110px 110px 110px;
      }

      .attention-banner .banner__text {
        padding-left: 100px;
        padding-right: 100px;
      }
    }

    /** TESTIMONIES ***************************************************************/

    .testimony-list {
      list-style-type: none;
      margin: 0;
      padding: 60px 0 0 0;
    }

    .testimony-list:after {
      content: "";
      clear: both;
      display: table;
    }

    .testimony-list__item {
      display: block;
      margin: 0;
      padding: 0;
    }

    .testimony {
      background-color: #f2f2f2;
      color: #565656;
      display: block;
      margin: 20px 40px 80px 20px;
      padding: 25px 25px 50px 25px;
      position: relative;
    }

    .testimony:after {
      content: '”';
      background-color: #090e88;
      background-image: radial-gradient(circle at center, #242ca8, #080e6f);
      border-radius: 100%;
      bottom: -40px;
      color: white;
      display: block;
      font-family: 'Nobile', serif;
      font-size: 100px;
      height: 80px;
      line-height: 150px;
      overflow: hidden;
      position: absolute;
      right: -20px;
      text-align: center;
      width: 80px;
    }

    .testimony__text {
    }

    .testimony__text p {
      padding: 5px 0;
    }

    .testimony__author {
      color: #080e6f;
      font-weight: 600;
      margin-top: 12px;
    }

    @media(min-width: 650px) {
      .testimony-list__item {
        float: left;
        width: 50%;
      }
      .testimony-list__item:nth-child(odd) {
        clear: both;
      }
    }

    @media(min-width: 1100px) {
      .testimony-list__item {
        width: 25%;
      }
      .testimony-list__item:nth-child(odd) {
        clear: none;
      }
    }

    /** INTRO BANNER **************************************************************/

    .intro-banner {
      margin: 40px 0;
      padding: 25px 30px 25px 30px;
      position: relative;
    }

    .intro-banner:after {
      content: "";
      clear: both;
      display: table;
    }

    .intro-banner .banner__image {
      background-image: url(data:image/jpeg;base64,);
      background-position: center 10%;
      background-size: cover;
      display: block;
      height: 200px;
      margin: 0 -30px 25px;
    }

    @media(min-width: 400px) {
      .intro-banner .banner__image {
        height: 250px;
      }
    }

    @media(min-width: 500px) {
      .intro-banner .banner__image {
        height: 300px;
      }
    }

    @media(min-width: 600px) {
      .intro-banner {
        padding-right: 300px;
      }

      .intro-banner .banner__image {
        bottom: -20px;
        height: auto;
        margin: 0;
        position: absolute;
        right: 15px;
        top: -20px;
        width: 250px;
      }
    }

    @media(min-width: 900px) {
      .intro-banner {
        padding: 40px 350px 50px 50px;
      }

      .intro-banner .banner__image {
        right: 30px;
        width: 300px;
      }
    }

    @media(min-width: 1100px) {
      .intro-banner {
        padding: 60px 510px 80px 100px;
      }

      .intro-banner .banner__image {
        right: 60px;
        width: 400px;
      }
    }

    /** PREDICTIONS ***************************************************************/

    .prediction-container {
      padding: 20px 0 150px 0;
    }

    .prediction-calendar {
      display: block;
      list-style-type: none;
      margin: 0;
      padding: 50px 0;
      position: relative;
    }

    .prediction-calendar__item {
      clear: both;
      padding: 15px 0;
      position: relative;
    }

    .prediction-calendar__item:after {
      content: "";
      clear: both;
      display: table;
    }

    .prediction-calendar__marker {
      background-image: radial-gradient(circle at center, #c0e78b, #8dc63f);
      border-radius: 100%;
      color: white;
      font-weight: 700;
      line-height: 65px;
      height: 65px;
      position: absolute;
      right: 10px;
      text-align: center;
      top: 15px;
      width: 65px;
    }

     .prediction-calendar__marker--double {
       line-height: 1.2;
       padding: 15px 0;
     }

    .prediction-calendar__text {
      background-color: #f2f2f2;
      color: #565656;
      margin-right: 95px;
      padding: 12px;
      position: relative;
    }

    .prediction-calendar__text:after {
      content: "";
      border: 9px solid #f2f2f2;
      border-bottom-color: transparent;
      border-top-color: transparent;
      border-right-style: none;
      display: block;
      height: 0;
      position: absolute;
      right: -9px;
      top: 24px;
      width: 0;
    }

    .prediction-calendar__item:nth-child(even) .prediction-calendar__marker {
      background-image: radial-gradient(circle at center, #242ca8, #080e6f);
      left: 10px;
      right: initial;
    }

    .prediction-calendar__item:nth-child(even) .prediction-calendar__text {
      margin-left: 95px;
      margin-right: 0;
    }

    .prediction-calendar__item:nth-child(even) .prediction-calendar__text:after {
      border-left-style: none;
      border-right-style: solid;
      left: -9px;
      right: initial;
    }

    @media(min-width: 650px) {
      .prediction-container {
        padding-left: 25px;
        padding-right: 25px;
      }

      .prediction-calendar:before {
        content: "";
        background-color: #898989;
        bottom: 50px;
        left: 50%;
        margin-left: -1px;
        position: absolute;
        top: 50px;
        width: 2px;
      }

      .prediction-calendar__item:nth-child(even) .prediction-calendar__marker,
      .prediction-calendar__item:nth-child(odd) .prediction-calendar__marker {
        left: 50%;
        margin-left: -32.5px;
        right: initial;
      }

      .prediction-calendar__container {
        width: 50%;
      }

      .prediction-calendar__item:nth-child(even) .prediction-calendar__container {
        float: right;
        padding-left: 60px;
      }

      .prediction-calendar__item:nth-child(odd) .prediction-calendar__container {
        padding-right: 60px;
      }

      .prediction-calendar__item:nth-child(even) .prediction-calendar__text,
      .prediction-calendar__item:nth-child(odd) .prediction-calendar__text {
        margin: 0;
      }
    }

    @media(min-width: 1100px) {

      .prediction-container {
        padding-left: 80px;
        padding-right: 80px;
      }

      .prediction-calendar__text {
        padding: 36px;
      }
    }

    /** CLOSING BANNER ************************************************************/

    .closing-banner {
      padding: 50px 25px;
    }

    @media(min-width: 650px) {
      .closing-banner {
        padding: 50px 100px;
      }
    }


    @media(min-width: 1100px) {
      .closing-banner {
        padding: 50px 200px;
      }

      .closing-banner .banner__text {
        padding: 30px 0;
      }
    }

    /** FOOTER ********************************************************************/

    .footer {
      background-color: #ccc;
      padding: 25px 25px 80px 25px;
      position: relative;
    }

    .copyright-notice {
      color: white;
    }

    .social-menu {
      bottom: 20px;
      position: absolute;
      right: 25px;
      text-align: right;
    }

    .social-menu .menu__item {
      padding-left: 15px;
    }

    .social-menu .menu__link {
      background-color: #777576;
      border-radius: 100%;
      color: #ccc;
      display: block;
      font-size: 25px;
      height: 40px;
      line-height: 40px;
      text-align: center;
      text-decoration: none;
      width: 40px;
    }

    .disclaimer {
      font-size: 0.8em;
      padding: 40px 20px;
    }

    .disclaimer p {
      margin: 5px 0;
    }

    @media(min-width: 450px) {
      .footer {
        padding-bottom: 25px;
        padding-right: 150px;
      }

      .social-menu {
        bottom: initial;
        top: 20px;
      }
    }

    @media(min-width: 650px) {
      .footer {
        padding: 30px 180px 30px 50px;
      }

      .social-menu {
        right: 50px;
        top: 20px;
      }
    }
  </style>
</head>
